{
    "plugins": [
           "@typescript-eslint",
           "prettier",
           "import",
           "node"
     ],
     "extends": [
           "plugin:@typescript-eslint/recommended",
           "plugin:prettier/recommended",
           "prettier"
      ],
     "parserOptions": {
       "project": "./tsconfig.json",
       "ecmaVersion": 2020,
       "sourceType": "module"
     },
     "env": {
           "es6": true,
           "node": true
       },
     "rules": {
       "no-debugger": "off",
       "no-console": 0,
       "import/no-extraneous-dependencies": ["off", {"devDependencies": ["**/*.test.ts", "**/*.spec.ts"]}]
     }
   }
   